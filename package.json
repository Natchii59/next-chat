{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo build",
    "clean": "turbo clean",
    "dev": "turbo dev --no-cache  --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "test": "turbo test"
  },
  "devDependencies": {
    "next": "^13.4.10",
    "prettier": "^3.0.0",
    "prettier-config": "workspace:*",
    "turbo": "^1.10.7"
  },
  "packageManager": "pnpm@8.6.7"
}
